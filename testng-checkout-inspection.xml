<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Checkout Inspection Suite">
    <test name="Android Checkout Inspection">
        <parameter name="platform" value="android"/>
        <classes>
            <class name="com.saucelabs.tests.CheckoutInspectionTest"/>
        </classes>
    </test>
    <test name="iOS Checkout Inspection">
        <parameter name="platform" value="ios"/>
        <classes>
            <class name="com.saucelabs.tests.CheckoutInspectionTest"/>
        </classes>
    </test>
</suite>
